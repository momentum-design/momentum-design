---
import themeStableDark from '@momentum-design/tokens/dist/json/theme/webex/dark-stable.json';
import themeStableDarkBronze from '@momentum-design/tokens/dist/json/theme/webex/dark-bronze-stable.json';
import themeStableDarkIndigo from '@momentum-design/tokens/dist/json/theme/webex/dark-indigo-stable.json';
import themeStableDarkJade from '@momentum-design/tokens/dist/json/theme/webex/dark-jade-stable.json';
import themeStableDarkLavender from '@momentum-design/tokens/dist/json/theme/webex/dark-lavender-stable.json';
import themeStableDarkRose from '@momentum-design/tokens/dist/json/theme/webex/dark-rose-stable.json';

import themeStableLight from '@momentum-design/tokens/dist/json/theme/webex/light-stable.json';
import themeStableLightBronze from '@momentum-design/tokens/dist/json/theme/webex/light-bronze-stable.json';
import themeStableLightIndigo from '@momentum-design/tokens/dist/json/theme/webex/light-indigo-stable.json';
import themeStableLightJade from '@momentum-design/tokens/dist/json/theme/webex/light-jade-stable.json';
import themeStableLightLavender from '@momentum-design/tokens/dist/json/theme/webex/light-lavender-stable.json';
import themeStableLightRose from '@momentum-design/tokens/dist/json/theme/webex/light-rose-stable.json';

import themeHCStableDark from '@momentum-design/tokens/dist/json/theme/webex/hc-dark-stable.json';
import themeHCStableLight from '@momentum-design/tokens/dist/json/theme/webex/hc-light-stable.json';

import { ThemeTokensWrapper } from '../../../components/ThemeTokensWrapper/ThemeTokensWrapper';
import AssetsDetailLayout from '../../../layouts/AssetsDetailLayout.astro';
import './shared-styles.css';

---

<AssetsDetailLayout frontmatter={{ description: '', title: 'Theme Tokens', layout: '' }}>
  <a class="backLink" href="/momentum-design/en/tokens#theme-stable">
    <span>&lt; Tokens</span>
  </a>
  <h1 class="heading1">Theme Tokens Stable</h1>

  <ThemeTokensWrapper
    client:load
    themeTokens={{
      dark: { data: themeStableDark, path: 'theme/webex/dark-stable.json' },
      darkBronze: { data: themeStableDarkBronze, path: 'theme/webex/dark-bronze-stable.json' },
      darkIndigo: { data: themeStableDarkIndigo, path: 'theme/webex/dark-indigo-stable.json' },
      darkJade: { data: themeStableDarkJade, path: 'theme/webex/dark-jade-stable.json' },
      darkLavender: { data: themeStableDarkLavender, path: 'theme/webex/dark-lavender-stable.json' },
      darkRose: { data: themeStableDarkRose, path: 'theme/webex/dark-rose-stable.json' },
      light: { data: themeStableLight, path: 'theme/webex/light-stable.json' },
      lightBronze: { data: themeStableLightBronze, path: 'theme/webex/light-bronze-stable.json' },
      lightIndigo: { data: themeStableLightIndigo, path: 'theme/webex/light-indigo-stable.json' },
      lightJade: { data: themeStableLightJade, path: 'theme/webex/light-jade-stable.json' },
      lightLavender: { data: themeStableLightLavender, path: 'theme/webex/light-lavender-stable.json' },
      lightRose: { data: themeStableLightRose, path: 'theme/webex/light-rose-stable.json' },
      hcDark: { data: themeHCStableDark, path: 'theme/webex/hc-dark-stable.json' },
      hcLight: { data: themeHCStableLight, path: 'theme/webex/hc-light-stable.json' },
    }}
  />
</AssetsDetailLayout>
