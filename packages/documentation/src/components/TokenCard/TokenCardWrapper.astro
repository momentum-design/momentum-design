---
import typographyTokens from '@momentum-design/tokens/dist/json/typography/complete.json';
import TokenCard from './TokenCard.astro';
import './TokenCard.css';
---

<div class="tokenCardWrapper">
  {
    Object.keys(typographyTokens).map((token) => (
      <TokenCard name={token} link={`/en/tokens/${token}`} />
    ))
  }
  <TokenCard name="Elevation" link={'/en/tokens/elevation'} />
  <TokenCard name="Effect" link={'/en/tokens/effect'} />
</div>
