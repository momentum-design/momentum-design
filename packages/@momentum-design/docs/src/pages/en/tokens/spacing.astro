---
import tokens from '@momentum-design/tokens/dist/json/core/complete.json';

import TokenTable, { UnthemedColorToken } from "../../../components/Tokens/TokenTable/TokenTable.astro";
import MainLayout from "../../../layouts/MainLayout.astro";

const spacingTokens = tokens.spacing as UnthemedColorToken;

---

<MainLayout frontmatter={{ description: "", title: "Spacing Tokens", layout: "" }} headings={[]}>
  <a href="../tokens"><h3>Back</h3></a>
  <TokenTable name="Spacing" headings={["Token", "Value"]} tokens={{spacing: spacingTokens}}/>
</MainLayout>
