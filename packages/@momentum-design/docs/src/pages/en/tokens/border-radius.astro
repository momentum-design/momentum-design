---
import tokens from '@momentum-design/tokens/dist/json/core/complete.json';

import TokenTable, { UnthemedColorToken } from "../../../components/Tokens/TokenTable/TokenTable.astro";
import MainLayout from "../../../layouts/MainLayout.astro";

const borderRadiusTokens = tokens['border-radius'] as UnthemedColorToken;

---

<MainLayout frontmatter={{ description: "", title: "Border Radius Tokens", layout: "" }} headings={[]}>
  <a href="../tokens"><h3>Back</h3></a>
  <TokenTable name="Border Radius" headings={["Token", "Value"]} tokens={{'border-radius': borderRadiusTokens}}/>
</MainLayout>
