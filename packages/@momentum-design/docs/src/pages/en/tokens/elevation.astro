---
import tokens from '@momentum-design/tokens/dist/json/core/complete.json';

import TokenTable, { UnthemedColorToken } from "../../../components/Tokens/TokenTable/TokenTable.astro";
import MainLayout from "../../../layouts/MainLayout.astro";

const elevationTokens = tokens['elevation'] as unknown as UnthemedColorToken;

---

<MainLayout frontmatter={{ description: "", title: "Elevation Tokens", layout: "" }} headings={[]}>
  <a href="../tokens"><h3>Back</h3></a>
  <TokenTable name="Elevation" headings={["Token", "Value"]} tokens={{'elevation': elevationTokens}}/>
</MainLayout>
